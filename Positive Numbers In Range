def print_positive_numbers(start, end):
    if start < 0:
        start = 0
    for num in range(start, end + 1):
        print(num)


start_num = int(input("Enter the start of the range: "))
end_num = int(input("Enter the end of the range: "))

print("Positive numbers in the range:")
print_positive_numbers(start_num, end_num)
